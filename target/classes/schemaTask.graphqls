type Task{
    id: Int!
    title: String!,
    description: String!,
    date: String!,
    status: status!
}
input TaskInput{
    id: Int,
    title:String!
    description:String!,
    date: String!,
    status: status!
}
enum status{
    open
    inProgress
    closed
}

type Query{
    getAll: [Task!]!,
    getAllWithFilter(contains:String!):[Task!]!
}

type Mutation{
    create(Task:TaskInput!): Task
}




